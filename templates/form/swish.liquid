<div class="container">
  <form method="post" autocomplete="on" action="process_swish">
    <input type="hidden" name="session_id" value="{{ model.session_id }}" />
    <div class="text-center">
      <img src="/assets/v2/images/brands/swish.svg" alt="Swish logo">
    </div>
    <div class="form-group">
      <label for="mobile">{% t Mobile number %}</label>
      <input
        type="tel"
        id="mobile"
        name="mobile"
        class="form-control"
        autocomplete="tel"
        pattern="^\s*(?:\+?(\d{1,9}))?[-. (]*[0-9\s]*"
        inputmode="numeric"
        value="{{ model.mobile_number }}"
        placeholder="07XXXXXX"/>
    </div>
    <button type="submit" class="btn btn-primary">{% t Proceed %}</button>
  </form>
</div>